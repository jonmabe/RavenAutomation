# Voice Backend Configuration
# Choose between 'openai' or 'vapi'
VOICE_BACKEND=openai

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_VOICE=ballad
OPENAI_MODEL=gpt-4o-realtime-preview-2024-12-17

# VAPI Configuration
VAPI_API_KEY=your_vapi_api_key_here
VAPI_PUBLIC_KEY=your_vapi_public_key_here
VAPI_ASSISTANT_ID=your_vapi_assistant_id_here

# VAPI Voice Provider Settings (optional)
VAPI_VOICE_PROVIDER=elevenlabs
VAPI_VOICE_ID=your_elevenlabs_voice_id_here
VAPI_VOICE_MODEL=eleven_turbo_v2

# VAPI Model Settings (optional)
VAPI_MODEL_PROVIDER=openai
VAPI_MODEL=gpt-4
VAPI_TEMPERATURE=0.9

# VAPI Transcriber Settings (optional)
VAPI_TRANSCRIBER_PROVIDER=deepgram
VAPI_TRANSCRIBER_MODEL=nova-2